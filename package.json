{
  "name": "velocity",
  "version": "0.1.2",
  "description": "[![Build Status](https://travis-ci.com/kx-chen/velocity.svg?token=shUduPKcLXQRPp5hZihq&branch=master)](https://travis-ci.com/kx-chen/velocity)",
  "private": true,
  "directories": {
    "lib": "lib"
  },
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "watch": "webpack --watch",
    "start": "webpack-dev-server --open",
    "build": "webpack --mode production",
    "lint": "npx prettier src/**/*.js  --check",
    "lint-fix": "npx prettier src/**/*.js  --write"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kx-chen/velocity.git"
  },
  "author": "Kai Chen",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/kx-chen/velocity/issues"
  },
  "homepage": "https://github.com/kx-chen/velocity#readme",
  "devDependencies": {
    "@babel/core": "^7.8.7",
    "@babel/preset-env": "^7.8.7",
    "@babel/preset-react": "^7.8.3",
    "babel-loader": "^8.3.0",
    "chai": "^4.2.0",
    "css-loader": "^6.7.3",
    "enzyme": "^3.11.0",
    "enzyme-adapter-preact-pure": "^2.2.0",
    "enzyme-to-json": "^3.4.4",
    "jest": "^25.1.0",
    "preact": "^10.3.4",
    "react-redux": "^7.2.0",
    "redux": "^4.0.5",
    "redux-mock-store": "^1.5.4",
    "redux-thunk": "^2.3.0",
    "style-loader": "^1.3.0",
    "webpack": "^5.75.0",
    "webpack-bundle-analyzer": "^3.6.1",
    "webpack-cli": "^5.0.1",
    "webpack-dev-server": "^3.10.3"
  },
  "jest": {
    "setupFilesAfterEnv": [
      "<rootDir>/src/js/setupTests.js"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "<rootDir>/src/js/__mocks__/styleMock.js",
      "^react$": "preact/compat",
      "^react-dom$": "preact/compat"
    },
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "modulePaths": [
      "<rootDir>"
    ]
  }
}
