{
  "name": "Velocity",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "A New Tab screen for humans. Simple, elegant and easy to use. Add your own custom backgrounds to make it your own. Free, forever.",
  "homepage_url": "http://extensionizr.com",
  "icons": {
    "64": "icons/icon64.png"
  },
  "default_locale": "en",
  "background": {
    "scripts": [
      "src/bg/background.js"
    ],
    "persistent": true
  },
  "options_page": "src/options_custom/index.html",
  "browser_action": {
    "default_icon": "icons/icon64.png",
    "default_title": "Velocity",
    "default_popup": "src/browser_action/browser_action.html"
  },
  "chrome_url_overrides": {
    "newtab": "src/override/override.html"
  },
  "permissions": [
    "storage"
  ]
}